    "import pandas as pd\n",
    "import pickle\n",
    "\n",
    "\n",
    "cnt = [0] * 5\n",
    "labels = pickle.load(open('simulated_data_label.pkl', 'rb'))\n",
    "for label in labels:\n",
    "    cnt[label] += 1\n",
    "\n",
    "for i in range(5):\n",
    "    print('Label {label} percentage:{percentage:8.2f}%'.format(label=i, percentage=cnt[i]/1200))\n"
   ]
  }
